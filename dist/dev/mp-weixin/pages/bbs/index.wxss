.big-box {
  background: rgb(241, 241, 241);
}
.trade {
  position: -webkit-sticky;
  position: sticky;
  top: 0rpx;
  z-index: 99;
  display: flex;
  justify-content: space-evenly;
  background: #fff;
  font-size: 32rpx;
}
.trade view {
  text-align: center;
  width: 50px;
}
.trade .texts.active {
  border-bottom: 8rpx solid #00c170;
  font-weight: bold;
  font-size: 36rpx;
}
.trade .texts {
  padding-bottom: 10rpx;
}
.sgscroll {
  height: 100vh;
}
.sglist {
  display: flex;
  flex-direction: column;
}
.sglist-item {
  margin-bottom: 15rpx;
  background: #fff;
  box-shadow: 0rpx 5rpx 10rpx #d7d7d7;
}
.sglist-item .sglist-view {
  width: 95vw;
  margin: 0 auto;
  padding: 20rpx 10rpx;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}
.sglist-item .sglist-title {
  width: 600rpx;
  font-size: 36rpx;
  font-weight: 700;
}
.sglist-item .sglist-user {
  margin-top: 10rpx;
  font-weight: bold;
  color: rgb(75, 75, 75);
  font-size: 24rpx;
}
.sglist-item .sglist-desc {
  font-size: 28rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.sglist-item .sglist-desc .sglist-content {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  padding-right: 10rpx;
}
.sglist-item .sglist-desc .sglist-imglist image {
  width: 150rpx;
  height: 110rpx;
  border-radius: 10rpx;
}
.sglist-item .sglist-flow {
  color: rgb(157, 157, 157);
  font-size: 26rpx;
  font-weight: 500;
}
.sglist-item .sglist-flow text {
  margin-right: 20rpx;
}
.addbtn {
  position: fixed;
  bottom: 20rpx;
  right: 30rpx;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 90rpx 90rpx;
  width: 90rpx;
  height: 90rpx;
  border-radius: 60rpx;
}
.navigator-hover {
  background-color: rgba(205, 205, 205, 0.171);
  opacity: 0.9;
}
.popup-content {
  height: 50vh;
  background: #ffffff;
  border-radius: 35rpx 35rpx 0 0;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: flex-start;
  flex-wrap: wrap;
}
.popup-content view:nth-child(1),
.popup-content view:nth-child(2) {
  height: 180rpx;
  width: 300rpx;
  border-radius: 30rpx;
  margin-top: 50rpx;
  padding-left: 30rpx;
  padding-top: 30rpx;
  box-shadow: 2rpx 7rpx 10rpx rgb(184, 184, 184);
}
.popup-content view:nth-child(1) text,
.popup-content view:nth-child(2) text {
  line-height: 40rpx;
}
.popup-content view:nth-child(1) text:nth-child(1),
.popup-content view:nth-child(2) text:nth-child(1) {
  font-size: 32rpx;
  color: #fff;
  font-weight: 600;
}
.popup-content view:nth-child(1) text:nth-child(2),
.popup-content view:nth-child(2) text:nth-child(2) {
  font-size: 24rpx;
  color: #e8e8e8;
}
.popup-content view:nth-child(3),
.popup-content view:nth-child(4),
.popup-content view:nth-child(5) {
  height: 150rpx;
  width: 170rpx;
  border-radius: 30rpx;
}
.popup-content .ask {
  display: flex;
  flex-direction: column;
  background: url(data:image/png;base64,);
  background-color: #1296db;
  background-size: 100rpx 100rpx;
  background-repeat: no-repeat;
  background-position: right bottom;
}
.popup-content .reply {
  display: flex;
  flex-direction: column;
  background: url(data:image/png;base64,);
  background-color: #00c170;
  background-size: 100rpx 100rpx;
  background-repeat: no-repeat;
  background-position: right bottom;
}
.popup-content .video image {
  width: 90rpx;
  height: 90rpx;
}
.popup-content .essay image {
  width: 90rpx;
  height: 90rpx;
}
.popup-content .sell image {
  width: 90rpx;
  height: 90rpx;
}
.popup-content .small {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  font-weight: 600;
}
